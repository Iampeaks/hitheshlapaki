 \n\n\n    const register = e => {\n        e.preventDefault();\n\n        auth\n            .createUserWithEmailAndPassword(email, password)\n            .then((auth) => {\n                // it successfully created a new user with email and password\n                if (auth) {\n                    history.push('/')\n                }\n            })\n            .catch(error => alert(error.message))\n    }\n\n    const signIn = e => {\n        e.preventDefault();\n\n        auth\n            .signInWithEmailAndPassword(email, password)\n            .then(_auth => {\n                history.push('/')\n            })\n            .catch(error => alert(error.message))\n    }\n\n    \n    return (\n        <div className='login'>\n            <Link to='/'>\n                <img\n                    className=\"login__logo\"\n                    src={logo} \n                />\n            </Link>\n\n            <div className='login__container'>\n                <h1>Sign-in</h1>\n\n                <form>\n                    <h5>E-mail</h5>\n                    <input type='text' value={email} onChange={e => setEmail(e.target.value)} />\n\n                    <h5>Password</h5>\n                    <input type='password' value={password} onChange={e => setPassword(e.target.value)} />\n\n                    <button type='submit' onClick={signIn} className='login__signInButton'>Sign In</button>\n                </form>\n\n                <p>\n                By signing-in you agree to the Rahul's Conditions of Use & Sale. \n                Please see our Privacy Notice, \n                our Cookies Notice and our Interest-Based Hithesh Adds. \n                Lapaki Hithesh.\n                </p>\n\n                <button onClick={register} className='login__registerButton'>Create your Amazon Account</button>\n            </div>\n        </div>\n    )\n}\n\nexport default Login","/Volumes/GK's Macbook/HITHESH_WEB_APP/hithesh-app/src/firebase.js",[],{"ruleId":"73","sev